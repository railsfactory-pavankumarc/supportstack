<section class="well well-small mePadd">
  <h2 class="meFont" style="padding-bottom:30px;"><span class="icon-comment meMarg"></span> Log in</h2>
  <div class="">

<% if notice %>
        <p class="alert alert-success"><%= notice %></p>
        <% end %>
       <% if alert %>
         <p class="alert alert-danger"><%= alert %></p>
        <% end %>

<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "form-actions form-horizontal"}) do |f| %>
<%= devise_error_messages! %>

<div class="control-group">
<label class="control-label" for="">Email:</label>
<div class="controls">
  <%= f.email_field :email, autofocus: true, class: "input-block-level" %>
</div>
</div>

<div class="control-group">
<label class="control-label" for="">Password:</label>
<div class="controls">
<%= f.password_field :password, autocomplete: "off", class: "input-block-level" %>
</div>
</div>
 
<div class="control-group pull-right">
<div class="controls">
<%= f.submit "Log in" %>
</div>
</div>
<%end%>
<%= render "devise/shared/links" %>


</div>
</section>